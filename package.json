{
  "name": "project-management-app",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://alexborisewich.github.io/project-management-app/",
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.10.14",
    "@mui/lab": "^5.0.0-alpha.108",
    "@mui/material": "^5.10.14",
    "@mui/x-data-grid-generator": "^5.17.13",
    "@reduxjs/toolkit": "^1.8.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^27.5.2",
    "@types/node": "^18.8.4",
    "@types/react": "^18.0.19",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^0.27.2",
    "framer-motion": "^7.6.7",
    "i18next": "^22.0.6",
    "jwt-decode": "^3.1.2",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.39.4",
    "react-i18next": "^12.0.0",
    "react-redux": "^8.0.2",
    "react-responsive": "^9.0.2",
    "react-router-dom": "^6.4.0",
    "react-scripts": "5.0.1",
    "react-toastify": "^9.1.1",
    "styled-components": "^5.3.6",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build && echo '/* /index.html  200' | cat >build/_redirects ",
    "test": "craco test",
    "test-coverage": "npm run test -- --coverage --watch=false",
    "eject": "react-scripts eject",
    "lintcode": "eslint --ext .js,.jsx,.ts,.tsx src",
    "fixcode": "npm run lintcode -- --fix",
    "lintstyles": "stylelint ./src/**/*.{css,scss,sass}",
    "fixstyles": "npm run lintstyles -- --fix",
    "lint": "npm run lintcode && npm run lintstyles",
    "fix": "npm run fixcode && npm run fixstyles",
    "prepare": "husky install",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build -m \"feat: deploy app to github pages\"",
    "new-component": "hygen new component"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^7.0.0",
    "@types/react-beautiful-dnd": "^13.1.2",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^4.0.0",
    "husky": "^8.0.0",
    "hygen": "^6.2.11",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "stylelint": "^14.13.0",
    "stylelint-config-clean-order": "^0.9.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^28.0.0",
    "typescript": "^4.8.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/index.{ts, tsx}",
      "!src/**/*.types.ts",
      "!src/**/*.d.ts",
      "!src/index.tsx",
      "!src/serviceWorker.ts",
      "!src/reportWebVitals.ts"
    ]
  }
}
